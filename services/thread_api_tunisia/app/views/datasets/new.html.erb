<h1>New dataset</h1>

<% form_for(@dataset) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :scrape_type %><br />
    <%= f.text_field :scrape_type %>
  </p>
  <p>
    <%= f.label :start_time %><br />
    <%= f.datetime_select :start_time %>
  </p>
  <p>
    <%= f.label :length %><br />
    <%= f.text_field :length %>
  </p>
  <p>
    <%= f.label :created_at %><br />
    <%= f.datetime_select :created_at %>
  </p>
  <p>
    <%= f.label :updated_at %><br />
    <%= f.datetime_select :updated_at %>
  </p>
  <p>
    <%= f.label :scrape_finished %><br />
    <%= f.check_box :scrape_finished %>
  </p>
  <p>
    <%= f.label :instance_id %><br />
    <%= f.text_field :instance_id %>
  </p>
  <p>
    <%= f.label :params %><br />
    <%= f.text_field :params %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', datasets_path %>